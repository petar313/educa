<div>
    {% if materials is empty %}
            <div class="alert alert-danger">Nema materijala za preuzimanje.</div>   
    {%else%}
        {%for material in materials%}
            <div class="list-group list-group-item  list-group-item-info"><img src="{{ asset("bundles/app/images/material/#{material.extension}.png")}}" style="width: 10%; margin-right: 10px"> <a href="{{path('show_material',{"material":material.id})}}" class="d2">{{material.name}}.{{material.extension}}</a></div> 
        {%endfor%}
    {%endif%}
</div>